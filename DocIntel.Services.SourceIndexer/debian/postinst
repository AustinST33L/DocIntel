#!/bin/bash
#
set -e

. /usr/share/debconf/confmodule

chown -R docintel:docintel /usr/lib/docintel/source-indexer/

cp /usr/share/doc/docintel/nlog.config.worker-example /usr/lib/docintel/source-indexer/nlog.config
sed -i "s,WORKER_NAME,source-indexer,g" /usr/lib/docintel/source-indexer/nlog.config

exit 0


